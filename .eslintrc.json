{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-console": "off",
        "import/prefer-default-export": 0,
        "no-unused-vars": "off",
        "object-curly-newline": "off",
        "indent": "off",
        "newline-per-chained-call": "off",
        "no-plusplus": "off",
        "no-shadow": "off",
        "func-names": "off",
        "class-methods-use-this": "off"
    }
}
