{
  "name": "customerapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test npm run eslint:fix && npx jest -i",
    "build": "NODE_ENV=production rm -rf ./dist && npx ttsc -P ./tsconfig.json --outDir dist && babel dist -d dist",
    "start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/server.js",
    "dev": "NODE_ENV=development npx nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node -r tsconfig-paths/register --files src/server.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run build && npm run typeorm migration:generate --",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "eslint": "npx eslint src",
    "eslint:fix": "npx eslint --fix src",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "git-commit-msg-linter": "^4.1.0",
    "pg": "^8.7.3",
    "pretty-quick": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.44"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/pg": "^8.6.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "tsconfig-paths": "^3.12.0",
    "ttypescript": "^1.5.13"
  }
}
